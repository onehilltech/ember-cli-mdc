<div class="mdc-dialog" ...attributes
  {{did-insert this.didInsert}}
  {{did-update this.open @open}}>
  <div class="mdc-dialog__container">
    <div class="mdc-dialog__surface"
         role="alertdialog"
         aria-modal="true"
        {{did-insert this.prepareDialogSurface}}>

      {{yield}}
    </div>
  </div>
  <div class="mdc-dialog__scrim"></div>
</div>
